%body
  %section
    .contact-info
      %ul
        - data.contact.each do |name|
          %li= name[:address]
          %li= name[:email]
          %li= name[:phone]
    .profile-image
  .education
    %h2 education
    %ul
      - data.education.each do |education|
        %li= education[:education]
        %p= education[:description]
  .skills
    %h2 Hard Skills
    %ul
      - data.skills.each do |skills|
        %li= skills[:hard_skills]
    %h2 Soft Skills
    %ul
      - data.skills.each do |skills|
        %li= skills[:soft_skills]
